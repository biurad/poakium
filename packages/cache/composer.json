{
    "name": "biurad/biurad-caching",
    "type": "library",
    "description": "Cache manager using PSR-6, with easy-to-use API for quick caching.",
    "keywords": [
        "caching",
        "cache",
        "psr6",
        "doctrine",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "require": {
        "php": "^7.1.3",
        "psr/cache": "^1.0",
        "doctrine/cache": "^1.9",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "cache/integration-tests": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Cache\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Cache\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/nette-di-bridge": "For autowiring the dependency in container"
    }
}
