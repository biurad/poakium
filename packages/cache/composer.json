{
  "name": "biurad/cache",
  "description": "A library that provides an advanced caching system for PSR-6 and PSR-16",
  "keywords": ["biurad","biurad","php","cache","caching","PSR-16","PSR-16"],
  "homepage": "https://biurad.com",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Divine Niiquaye Ibok",
      "email": "divineibok@gmail.com"
    },
    {
      "name": "GitHub Contributors",
      "homepage": "https://github.com/biurad/php-cache/contributors"
    }
  ],
  "support": {
    "docs": "https://docs.biurad.com/poakium/cache"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": "^8.0",
    "psr/cache": "^2.0|^3.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "doctrine/cache": "^2.0",
    "symfony/cache": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Biurad\\Cache\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "suggest": {
    "symfony/cache": "For using AdapterFactory class to create a PSR-6 doctrine cache adapter",
    "ext-memcached": "For caching into memcached storage",
    "ext-apcu": "For caching into apcu storage",
    "ext-redis": "For caching into redis storage",
    "ext-sqlite": "For caching into pdo sqlite storage"
  }
}
