{
    "name": "biurad/biurad-caching",
    "type": "library",
    "description": "Cache manager using Doctrine Cache offers a very intuitive PSR-16 and PSR-6 API for cache manipulation",
    "keywords": [
        "caching",
        "cache",
        "psr6",
        "psr16",
        "nette",
        "doctrine",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/projects/contributors"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "doctrine/cache": "^1.9",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0"
    },
    "sugguest": {
        "ext-memcached": "For caching into memcached storage",
        "ext-apcu": "For caching into apcu storage",
        "ext-redis": "For caching into redis storage",
        "ext-sqlite": "For caching into pdo sqlite storage",
        "cache/doctrine-adapter": "For PSR-6 cache implementation using Doctrine. This implementation supports tags"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Cache\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Cache\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
