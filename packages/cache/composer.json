{
    "name": "biurad/cache-manager",
    "type": "library",
    "description": "BiuradPHP Framework Cache manager with PSR-6, PSR-16",
    "keywords": ["caching", "psr6", "doctrine"],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "require": {
        "php": "^7.1.3",
        "psr/cache": "~1.0",
	    "doctrine/cache": "^1.9",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "cache/integration-tests": "dev-master"
    },
    "conflict": {
        "symfony/var-dumper": "<3.4"
    },
    "autoload": {
        "psr-4": { "BiuradPHP\\Cache\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
	"autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Cache\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/dependency-injection": "For autowiring the dependency in container"
    }
}
