{
    "name": "biurad/biurad-scaffolder",
    "type": "library",
    "description": "The Scaffold Maker is the fastest way to generate the most common code you'll need in a PHP projects",
    "keywords": [
        "scaffolding",
        "scaffolder",
        "php code generator",
        "php generator",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "biurad/doctrine-annotations-bridge": "^0.1",
        "symfony/console": "^5.1",
        "doctrine/inflector": "^1.3",
        "nette/neon": "^3.0",
        "nette/php-generator": "^3.3",
        "nikic/php-parser": "^4.4",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "composer/package-versions-deprecated": "^1.8",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Scaffold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Scaffold\\Tests\\": "Tests/"
        }
    }
}
