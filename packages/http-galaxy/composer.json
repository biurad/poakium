{
    "name": "biurad/biurad-http",
    "type": "library",
    "description": "Http Pipeline: abstraction for HTTP request, response, and csp protection. Provides careful data sanitization and utility for URL and cookies manipulation.",
    "keywords": [
        "http",
        "request",
        "response",
        "csp",
        "url",
        "proxy",
        "cookies",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^6.5",
        "laminas/laminas-httphandlerrunner": "^1.1",
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Http\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Http\\Test\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
