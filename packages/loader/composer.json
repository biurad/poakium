{
  "name": "biurad/loader",
  "description": "This dependency is a robust package for loading files, classes, class aliases, annotations, and store data",
  "keywords": ["loader", "loader manager", "xml", "yaml", "ini", "csv", "namespace", "alias", "config", "biurad", "php"],
  "homepage": "https://biurad.com",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Divine Niiquaye Ibok",
      "email": "divineibok@gmail.com"
    },
    {
      "name": "GitHub Contributors",
      "homepage": "https://github.com/biurad/php-loader/contributors"
    }
  ],
  "support": {
    "docs": "https://docs.biurad.com/poakium/loader"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-tokenizer": "*"
  },
  "require-dev": {
    "symfony/yaml": "^6.0",
    "nette/neon": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Biurad\\Loader\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "suggest": {
    "symfony/yaml": "Support loading files in yml or yaml format, if native yaml extension is not available",
    "nette/neon": "Support loading files in neon format, since nette/neon is an advanced version of yaml"
  }
}
